<mujoco model="cube in bin scene">
   <compiler angle="radian" autolimits="true"/>

   <option iterations="3" ls_iterations="5" solver="Newton" tolerance="1e-8" timestep="0.001">
     <flag eulerdamp="disable"/>
   </option>

  <!-- Assets -->
  <include file="simpleTable/simpleTable_asset.xml"/>

  <!-- Scene -->
  <include file="scene.xml"/> 

  
  <!-- Scene Objects -->
  <worldbody>
    <camera name="fixed" pos="0 0 2" quat="1 0 0 0" fovy="45"/>

 

    <!-- Table -->
    <include file="simpleTable/simpleWoodTable_body.xml"/>

   
  </worldbody>



 
</mujoco>
